  },
  "scripts": {
    "start": "nx serve",
    "graph": "nx graph",
    "test:chope": "nx test @wotm/chope --coverage --forceExit",
    "test:event": "nx run @wotm/event-registration:test --coverage --forceExit",
    "test:space": "nx run @wotm/space:test --coverage --forceExit",
    "test:vehicle": "nx run @wotm/vehicle:test --coverage --forceExit",
    "test:drive": "nx run @wotm/drive:test --coverage --forceExit",
    "build:chope": "rm -rf build && nx build @wotm/chope --skip-nx-cache && resolve-tspaths",
    "build:push": "rm -rf build && nx build @wotm/push --skip-nx-cache && resolve-tspaths",
    "build:event": "rm -rf build && nx build @wotm/event-registration --skip-nx-cache && resolve-tspaths",
    "build:space": "rm -rf build && nx build @wotm/space --skip-nx-cache && resolve-tspaths",
    "build:vehicle": "rm -rf build && nx build @wotm/vehicle --skip-nx-cache && resolve-tspaths",
    "build:drive": "rm -rf build && nx build @wotm/drive --skip-nx-cache && resolve-tspaths",
    "build:panorama": "rm -rf build && nx build @ds/panorama-server && resolve-tspaths",
    "build:all": "rm -rf build && nx run-many --target=build --skip-nx-cache && resolve-tspaths",
    "dev:chope": "nx run @wotm/chope:dev",
    "dev:space": "nx run @wotm/space:dev",
    "dev:vehicle": "nx run @wotm/vehicle:dev",
    "dev:drive": "nx run @wotm/drive:dev",
    "dev:event": "nx run @wotm/event-registration:dev",
    "dev:push": "nx run @wotm/push:dev",
    "prod:chope": "node build/packages/chope/dev.js",
    "prod:local:chope": "node -r dotenv/config build/packages/chope/dev.js",
    "prod:space": "node build/packages/space/dev.js",
    "prod:local:space": "node -r dotenv/config build/packages/space/dev.js",
    "prod:vehicle": "node build/packages/vehicle/dev.js",
    "prod:local:vehicle": "node -r dotenv/config build/packages/vehicle/dev.js",
    "prod:drive": "NODE_CONFIG_DIR=./build/packages/drive/config/ node build/packages/drive/dev.js",
    "prod:local:drive": "NODE_CONFIG_DIR=./build/packages/drive/config/ node -r dotenv/config build/packages/drive/dev.js",
    "prod:event": "node build/packages/event-registration/dev.js",
    "prod:local:event": "node -r dotenv/config build/packages/event-registration/dev.js",
    "prod:push": "node build/packages/push/dev.js",
    "prod:local:push": "node -r dotenv/config build/packages/push/dev.js",
    "prod:panorama": "node build/packages/panorama-server/dev.js",
    "prod:local:panorama": "node -r dotenv/config build/packages/panorama-server/dev.js",
    "generate:app": "nx generate @wotm/starter:starter-generator",
    "generate:lib": "nx generate @wotm/starter:lib-generator",
    "lint-all": "nx run-many --all --target=lint",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "check-lint-fix": "eslint . --ext ts --ext tsx --ext js --fix",
    "pre-commit": "next lint",
    "pre-commit-lint": "lint-staged",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "prepare": "husky install",
    "clean": "lerna clean -y && rm -rf node_modules",
    "reset": "nx reset",
    "test:rsde_dtt": "nx test @wotm/rsde_dtt --coverage --forceExit",
    "build:rsde_dtt": "rm -rf build && nx build @wotm/rsde_dtt --skip-nx-cache && resolve-tspaths",
    "dev:rsde_dtt": "nx run @wotm/rsde_dtt:dev",
    "prod:rsde_dtt": "node build/packages/rsde_dtt/dev.js",
    "prod:local:rsde_dtt": "node -r dotenv/config build/packages/rsde_dtt/dev.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.40.0",

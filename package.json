    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "sequelize": "^6.21.4",
    "tslib": "^2.3.0",
    "umzug": "^3.1.1",
    "uuid": "^9.0.0",
    "web-push": "^3.5.0",
    "winston": "^3.3.3",
    "yaml": "^2.2.1",
    "yarn": "^1.22.19",
    "zod": "^3.19.1"
  },
